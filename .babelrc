{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "last 2 version, IE 11"
      }
    ]
  ],
  "plugins": [
    "syntax-async-functions",
    "@babel/plugin-proposal-optional-chaining",
    "transform-regenerator",
    "@babel/plugin-proposal-class-properties",
    "dynamic-import-webpack"
  ]
}